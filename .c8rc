{
  "check-coverage": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "exclude": ["/node_modules/", "/src/prettier-comments/"],
  "include": [
    "src/**/*.ts",
    "!src/prettier-comments/**/*.ts",
    "!src/common/backward-compatibility.ts"
  ],
  "reporter": ["lcov", "text"],
  "temp-dir": "./coverage/"
}
