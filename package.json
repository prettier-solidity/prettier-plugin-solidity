  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "codecov": "^3.6.5",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^26.0.1",
    "jest-mock-now": "^1.3.0",
    "jest-watch-typeahead": "^0.6.0"
