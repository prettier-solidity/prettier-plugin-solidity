      "name": "Klaus Hott Vidal"
    },
    {
      "email": "soham@zemse.in",
      "name": "Soham Zemse"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prettier-solidity/prettier-plugin-solidity/issues"
  },
  "homepage": "https://github.com/prettier-solidity/prettier-plugin-solidity#readme",
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.24.7",
    "@types/jest": "^29.5.13",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "c8": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "esm-utils": "^4.3.0",
    "esmock": "^2.6.7",
    "jest": "^29.7.0",
    "jest-light-runner": "^0.6.0",
    "jest-snapshot-serializer-ansi": "^2.1.0",
    "jest-snapshot-serializer-raw": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lines-and-columns": "^2.0.4",
    "prettier": "^3.3.3",
    "proxyquire": "^2.1.3",
    "solc": "^0.8.27",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@nomicfoundation/slang": "0.17.0",
    "@solidity-parser/parser": "^0.18.0",
